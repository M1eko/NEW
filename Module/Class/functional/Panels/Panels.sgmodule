#!name=Panels
#!desc=信息面板模块
[Panel]
# netspeed
NetSpeed = script-name=NetSpeed


# ip isp city
NetISP = script-name=NetISP,update-interval=-1

# ping
NetPing = script-name=NetPing,update-interval=-1

# cf speedtest
NetSpeed = script-name=NetSpeed,update-interval=-1

#ChatGPT Panel
CFGPT = script-name=CFGPT,update-interval=-1

# 详细的显示你当前的网络信息
NetInfoPanel=title="",content="",style=info,script-name=net-info-panel.js,update-interval=1


# 配置重载
配置重载 = title=配置重载,content=配置重载,style=info,script-name=配置重载,update-interval=-1

# 刷新DNS
刷新DNS = script-name=刷新DNS,update-interval=1

# D+检测
disney_check = script-name=disney_check,title=Disney+ 解锁检测,style=alert,content=请刷新面板,update-interval=21600

# nf检测
nf_check = script-name=nf_check, title="Netflix 解锁检测", content="请刷新", update-interval=1

# 奈飞、油管解锁检测
stream-checktools = script-name=stream-check, title="奈飞、油管解锁检测", content="请刷新面板", update-interval=-1

# 流媒体解锁检测
stream-all = script-name=stream-all, title="流媒体解锁检测", content="请刷新面板", update-interval=-1

[Script]
# netspeed
NetSpeed = type=generic,script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/modules/Panel/Net_Speed/Net_Speed.js,argument=title=NetSpeed&iconfast=bolt&iconmid=hare&iconslow=tortoise&colorlow=#06D6A0&colormid=#FFD166&colorhigh=#EF476F


# NetPing
NetPing = type=generic,timeout=2,script-path=https://raw.githubusercontent.com/Keywos/rule/key/JS/NetPing.js

# NetISP
NetISP = type=generic,timeout=3,script-path=https://raw.githubusercontent.com/Keywos/rule/key/JS/NetISP.js

# NetSpeed
NetSpeed = type=generic,timeout=60,script-path=https://raw.githubusercontent.com/Keywos/rule/key/JS/NetSpeed.js

# Chatgpt可用区域检测随可用性更换ICON
CFGPT = type=generic,timeout=6,script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/modules/Panel/CFGPT/CFGPT_2.js,argument=icon=lasso.and.sparkles&iconerr=xmark.seal.fill&icon-color=#336FA9&iconerr-color=#D65C51

a# Network Info Panel-CN
net-info-panel.js=script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/modules/Panel/Network-Info/Moore/net-info-panel_CN.js,type=generic,timeout=30

# 配置重载
配置重载=script-path=https://raw.githubusercontent.com/LucaLin233/Luca_Conf/main/Surge/JS/Reload.js,type=generic

# 刷新DNS
刷新DNS = type=generic,timeout=3,script-path=https://raw.githubusercontent.com/zZPiglet/Task/master/asset/flushDNS.js,argument=title=刷新DNS缓存&icon=repeat.circle.fill&color=#FF9F0A&server=false

# D+解锁检测
disney_check = type=generic,script-path=https://gist.githubusercontent.com/Hyseen/729fc4c3ac28c3f2e7c4a832d81780f3/raw/disney_check.js?version=1637153952428

# nf检测
nf_check = type=generic, script-path=https://gist.githubusercontent.com/Hyseen/b06e911a41036ebc36acf04ddebe7b9a/raw/nf_check.js?version=1633074636264, argument=title=Netflix 解锁检测

# 奈飞、油管解锁检测
stream-check = type=generic, timeout=15, script-path=https://raw.githubusercontent.com/LucaLin233/Luca_Conf/main/Surge/JS/stream_check.js

# 流媒体解锁检测
stream-all = type=generic, timeout=15, script-path=https://raw.githubusercontent.com/LucaLin233/Luca_Conf/main/Surge/JS/stream-all.js

[Host]
ip-api.com = 208.95.112.1
